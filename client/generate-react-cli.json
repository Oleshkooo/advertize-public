{
    "usesTypeScript": true,
    "usesCssModule": true,
    "cssPreprocessor": "scss",
    "testLibrary": "None",
    "component": {
        "default": {
            "customTemplates": {
                "component": "templates/TemplateName.tsx",
                "style": "templates/TemplateName.module.scss",
                "lazy": "templates/TemplateName.lazy.tsx",
                "index": "templates/index.ts"
            },
            "path": "src/components",
            "withStyle": true,
            "withTest": false,
            "withStory": false,
            "withLazy": true,
            "withIndex": true
        }
    }
}
